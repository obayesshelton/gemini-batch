{
    "name": "obayesshelton/gemini-batch",
    "description": "Laravel package for the Google Gemini Batch API with optional PrismPHP and Laravel AI SDK integration",
    "keywords": ["laravel", "gemini", "batch", "ai", "prism", "google"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Bayes-Shelton",
            "email": "oliver@obayesshelton.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "echolabsdev/prism": "^0.93",
        "laravel/pint": "^1.27",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0"
    },
    "suggest": {
        "echolabsdev/prism": "Required for Prism integration (addTextRequest, addStructuredRequest)"
    },
    "autoload": {
        "psr-4": {
            "ObayesShelton\\GeminiBatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ObayesShelton\\GeminiBatch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ObayesShelton\\GeminiBatch\\BatchServiceProvider"
            ],
            "aliases": {
                "GeminiBatch": "ObayesShelton\\GeminiBatch\\Facades\\GeminiBatch"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
